@{
    Layout = "~/Views/Shared/_LayoutAdminPage.cshtml";
}
@model List<Product>
<div class="container mt-5" style="display: grid; align-items: center; gap: 40px;">
    <a asp-area="Admin" asp-controller="Product" asp-action="Create" class="btn btn-success">Add Product</a>
    <table id="dTable" class="table table-striped" style="width: 100%;">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Title</th>
                <th scope="col">Price</th>
                <th scope="col">Description</th>
                <th scope="col">Category</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
           
        </tbody>
    </table>
</div>

@section Scripts{
    <script src="~/js/product.js"></script>
}


